    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm start",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "name": "Launch Extension",
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "npm",
            "request": "launch",
            "type": "extensionHost"
        },
        {
            "type": "java",
         "name": "Debug (Launch) makecall",
            "request": "launch",
            "mainClass": "com.cisco.webdialer.makecall",
            "projectName": "vumc_webdialer",
            "env": {
                "CUCM": "10.10.20.31",
                "PROXY_USER": "agent1",
                "PASSWORD": "cisco123",
                "USER_NAME": "agent1",
                "DEVICE_NAME": "SEP6001FFFF0000",
                "CALL_FROM": "CHANGEME",
                "CALL_TO": "CHANGEME"
            }
        }
    ]
}